<h1>タスク一覧</h1>
<%= link_to "投稿画面へ", new_task_path %>
<% @task.each do |task|  %>
<p>
    <%= task.title %>
    <%= link_to "詳細",task_path(user.id) %>
    <%= link_to "編集",edit_task_path(user.id) ,method: :get %>
    <%= link_to "削除",task_path(user.id) ,method: :delete, data: {confirm: "削除しますか？"}  %>
</p>

